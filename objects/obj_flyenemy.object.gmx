            <string>///Switch States
switch (state)
{
    case fly_e_state.idle:
    {
        hspd = 0;
        vspd = 0;
        xcomponent = 0
        ycomponent = 0
        motion_set(point_direction(x, y, x, y), 0)
        
        if (distance_to_object(obj_CubeBoi) &gt; 256) or obj_CubeBoi.y &gt; obj_flyenemy.y + 100
        {
        state = fly_e_state.chase;
        }
    }
    break;
    
    case fly_e_state.chase:
    {
            motion_set(point_direction(x, y, obj_CubeBoi.x, obj_CubeBoi.y), fly_speed)
            xcomponent = lengthdir_x(fly_speed, point_direction(x, y, obj_CubeBoi.x, obj_CubeBoi.y))
            ycomponent = lengthdir_y(fly_speed, point_direction(x, y, obj_CubeBoi.x, obj_CubeBoi.y))
            motion_set(point_direction(x, y, x, y), 0)
            if obj_CubeBoi.y-100 &lt; obj_flyenemy.y or  distance_to_object(obj_CubeBoi) &lt; 512
            {
            state = fly_e_state.idle 
            }
    }
    break;
}
