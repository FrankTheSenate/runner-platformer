            <kind>1</kind>
            <string>///Switch States
if distance_to_object(obj_CubeBoi) &gt; 400 and distance_to_object(obj_CubeBoi) &lt; 1024
{
    state = fly_e_state.chase
}

else if distance_to_object(obj_CubeBoi) &gt; 2048
{
    state = fly_e_state.idle

}
else if irandom(1000) &gt; 10
{
    state = fly_e_state.buzzing
    
}
else
{
    state = fly_e_state.attacking
}



switch (state)
{
    case fly_e_state.idle:
    {
        motion_set(point_direction(x, y, x, y), 0)
        hspd = 0;
        vspd = 0;
        xcomponent = 0
        ycomponent = 0
    }
    break;
    
    case fly_e_state.chase:
    {
            motion_set(point_direction(x, y, obj_CubeBoi.x, obj_CubeBoi.y - 100), fly_speed)
            xcomponent = lengthdir_x(fly_speed, point_direction(x, y, obj_CubeBoi.x, obj_CubeBoi.y))
            ycomponent = lengthdir_y(fly_speed, point_direction(x, y, obj_CubeBoi.x, obj_CubeBoi.y))
            motion_set(point_direction(x, y, x, y), 0)
            
             //Checks direction and flips sprite direction
        if sign(hspd) &lt;= 0 //check if moving left
        {
            image_xscale=1
        }
        else if sign(hspd) &gt;=0 //check if moving right
        {
          image_xscale=-1   
        }
    }
    
    break;
    
    case fly_e_state.attacking:
    {
        motion_set(point_direction(x, y, x, y), 0)
        xcomponent = 0
        ycomponent = 0
        vspd = 0
        hspd = 0
        instance_create(x,y,obj_enemy_bullet)
    }
    break;
    
    case fly_e_state.buzzing:
    {
        if distance_to_object(obj_flyenemy) &gt; 200
        {
        motion_set(point_direction(x, y, obj_CubeBoi.x + random_range(-50,50), obj_CubeBoi.y - 200) , fly_speed)
        xcomponent = lengthdir_x(fly_speed, point_direction(x, y, obj_CubeBoi.x, obj_CubeBoi.y))
        ycomponent = lengthdir_y(fly_speed, point_direction(x, y, obj_CubeBoi.x, obj_CubeBoi.y))
        motion_set(point_direction(x, y, x, y), 0)
        }
    }
    break;
}
