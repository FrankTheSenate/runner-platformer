            <string>///Movemint
//I copied this from you nick
//Get CubeBoi vspd
playervspd = obj_CubeBoi.vspd


//Check floor
if place_meeting(x,y+1, obj_Ground)
{
    grounded = true
}
else
{
    grounded = false
}

//Horizontal collisions
if place_meeting(x+hspd,y,obj_Ground) 
{
        if !place_meeting(x+sign(hspd),y,obj_Ground) 
        {
                 x += sign(hspd)
        }
        hspd = 0
}

//Gravity
vspd += grav

//Vertical Collisions
if place_meeting(x,y+vspd,obj_Ground) 
{
        if !place_meeting(x,y+sign(vspd),obj_Ground) 
        {
                 y += sign(vspd)
        }
        vspd = 0
}


//Enemy Collision
if (place_meeting(x,y,obj_CubeBoi))
{
    if (playervspd &gt; 0)
    {
        with (obj_CubeBoi) vspd = -jspd;   //Bounce off of enemy
        instance_destroy();
    }
    
    else
    {
        game_restart();
    }
}





hspd += xcomponent
vspd += ycomponent

x += hspd
y += vspd
