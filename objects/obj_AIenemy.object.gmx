            <string>///SwitchStates
switch (state)
{
    case e_state.idle:
    {
        hspd = 0;
        vspd = (min(7,vspd+0.05));   //you can change this into a grav var later
        if (distance_to_object(obj_CubeBoi) &lt; 128) state = e_state.chase;
    }
    break;
    
    case e_state.chase:
    {
        dir = sign(obj_CubeBoi.x - x);
        hspd = dir * accel;
        vspd = (min(7,vspd+0.05));
        if (distance_to_object(obj_CubeBoi) &gt; 164) state = e_state.idle;
    }
    break;
}
